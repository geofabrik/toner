{
  "variables": {
    "aws_access_key": "",
    "aws_secret_key": "",
    "database_url":"postgres:///osm"
  },
  "builders": [
    {
      "type": "amazon-ebs",
      "access_key": "{{user `aws_access_key`}}",
      "secret_key": "{{user `aws_secret_key`}}",
      "region": "us-east-1",
